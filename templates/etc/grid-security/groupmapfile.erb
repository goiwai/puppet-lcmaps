<%-
@pools.each do | pool |
  @vo_name=pool['vo']
  @group=pool['group']
  @role=pool['role']
  if @role == nil
    @role = "NULL"
  end
  @capability=pool['capability']
  if @capability == nil
    @capability = "NULL"
  end
-%>
<% if @role == nil -%>
"/<%=@vo_name%>/Role=<%=@role%>/Capability=<%=@capability%>" <%=@group%>
"/<%=@vo_name%>" <%=@group%>
<% else -%>
"/<%=@vo_name%>/Role=<%=@role%>/Capability=<%=@capability%>" <%=@group%>
"/<%=@vo_name%>/Role=<%=@role%>" <%=@group%>
<% end -%>
<%- 
end
-%>
