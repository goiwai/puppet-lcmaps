<%-
@pools.each do | pool |
  @vo_name=pool['vo']
  @pool_name=pool['name']
  @role=pool['role']
  if @role == nil
    @role = "NULL"
  end
  @capability=pool['capability']
  if @capability == nil
    @capability = "NULL"
  end
-%>
<% if @role == nil -%>
"/<%=@vo_name%>/Role=<%=@role%>/Capability=<%=@capability%>" .<%=@pool_name%>
"/<%=@vo_name%>" .<%=@pool_name%>
<% else -%>
"/<%=@vo_name%>/Role=<%=@role%>/Capability=<%=@capability%>" .<%=@pool_name%>
"/<%=@vo_name%>/Role=<%=@role%>" .<%=@pool_name%>
<% end -%>
<%- 
end
-%>
